<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <children>
        <Circle fx:id="circle" fill="BLUE" layoutX="400.0" layoutY="300.0" radius="30.0" />


        <Button layoutX="379.0" layoutY="19.0" onAction="#moveUp" prefHeight="26.0" prefWidth="64.0" text="Up" />
        <Button layoutX="378.0" layoutY="101.0" onAction="#moveDown" prefHeight="26.0" prefWidth="65.0" text="Down" />
        <Button layoutX="300.0" layoutY="60.0" onAction="#moveLeft" prefHeight="26.0" prefWidth="69.0" text="Left" />
        <Button layoutX="454.0" layoutY="60.0" onAction="#moveRight" prefHeight="26.0" prefWidth="77.0" text="Right" />


        <Button layoutX="300.0" layoutY="20.0" onAction="#moveUpLeft" prefHeight="26.0" prefWidth="73.0" text="UpLeft" />
        <Button layoutX="450.0" layoutY="20.0" onAction="#moveUpRight" prefHeight="26.0" prefWidth="84.0" text="UpRight" />
        <Button layoutX="300.0" layoutY="100.0" onAction="#moveDownLeft" text="DownLeft" />
        <Button layoutX="455.0" layoutY="99.0" onAction="#moveDownRight" text="DownRight" />


        <Button layoutX="411.0" layoutY="167.0" onAction="#setRedColor" prefHeight="26.0" prefWidth="120.0" text="Red" />
        <Button layoutX="293.0" layoutY="168.0" onAction="#cycleColor" prefHeight="26.0" prefWidth="113.0" text="Change Color" />


        <Button layoutX="295.0" layoutY="135.0" onAction="#changeBackgroundColor" prefHeight="26.0" prefWidth="237.0" text="Random Background" />

        <Button fx:id="jumpButton" layoutX="651.0" layoutY="73.0" onAction="#jumpButtonAction" text="Click me" />
    </children>
</AnchorPane>
